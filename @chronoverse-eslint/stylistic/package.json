{
  "name": "@chronoverse-eslint/stylistic",
  "version": "4.0.0",
  "private": false,
  "description": "Comprehensive ESLint stylistic rules configuration for JavaScript, TypeScript, JSX and more",
  "keywords": [
    "eslint",
    "eslintconfig",
    "stylistic",
    "formatting",
    "style",
    "typescript",
    "javascript",
    "jsx",
    "chronoverse",
    "lint",
    "linter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gratisvictory/chronoverse-eslint"
  },
  "license": "MIT",
  "author": {
    "name": "@gratisvictory",
    "email": "gratisvictory.dev@proton.me",
    "url": "https://github.com/gratisvictory"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/@types/index.d.ts"
    },
    "./stylistic-base": {
      "import": "./dist/stylistic-base.js",
      "types": "./dist/@types/stylistic-base.d.ts"
    },
    "./spacing-rules": {
      "import": "./dist/rules/spacing-rules.js",
      "types": "./dist/@types/rules/spacing-rules.d.ts"
    },
    "./line-breaks-rules": {
      "import": "./dist/rules/line-breaks-rules.js",
      "types": "./dist/@types/rules/line-breaks-rules.d.ts"
    },
    "./brackets-rules": {
      "import": "./dist/rules/brackets-rules.js",
      "types": "./dist/@types/rules/brackets-rules.d.ts"
    },
    "./indent-rules": {
      "import": "./dist/rules/indent-rules.js",
      "types": "./dist/@types/rules/indent-rules.d.ts"
    },
    "./quotes-rules": {
      "import": "./dist/rules/quotes-rules.js",
      "types": "./dist/@types/rules/quotes-rules.d.ts"
    },
    "./commas-rules": {
      "import": "./dist/rules/commas-rules.js",
      "types": "./dist/@types/rules/commas-rules.d.ts"
    },
    "./semis-rules": {
      "import": "./dist/rules/semis-rules.js",
      "types": "./dist/@types/rules/semis-rules.d.ts"
    },
    "./operators-rules": {
      "import": "./dist/rules/operators-rules.js",
      "types": "./dist/@types/rules/operators-rules.d.ts"
    },
    "./comments-rules": {
      "import": "./dist/rules/comments-rules.js",
      "types": "./dist/@types/rules/comments-rules.d.ts"
    },
    "./jsx-rules": {
      "import": "./dist/rules/jsx-rules.js",
      "types": "./dist/@types/rules/jsx-rules.d.ts"
    },
    "./types-rules": {
      "import": "./dist/rules/types-rules.js",
      "types": "./dist/@types/rules/types-rules.d.ts"
    },
    "./disallow-rules": {
      "import": "./dist/rules/disallow-rules.js",
      "types": "./dist/@types/rules/disallow-rules.d.ts"
    },
    "./misc-rules": {
      "import": "./dist/rules/misc-rules.js",
      "types": "./dist/@types/rules/misc-rules.d.ts"
    },
    "./all-rules": {
      "import": "./dist/rules/all-rules.js",
      "types": "./dist/@types/rules/all-rules.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/@types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run clean:all && bun build.js",
    "build:clean": "rimraf dist",
    "build:cli": "bun run clean:all && bun run builder -e src/index.js -o dist",
    "clean": "rimraf node_modules",
    "clean:all": "rimraf dist node_modules",
    "format:check": "prettier --check --config ../../prettier.config.js \"**/*.{js,json,json5,jsonc}\" --cache --cache-location=node_modules/.cache/.prettiercache",
    "format:fix": "prettier --write --config ../../prettier.config.js \"**/*.{js,json,json5,jsonc}\" --cache --cache-location=node_modules/.cache/.prettiercache --log-level=warn",
    "lint:build:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest build --config ./eslint.config.stylistic.js --outDir .eslint-stylistic && bunx serve .eslint-stylistic",
    "lint:check": "eslint -c ../../eslint.config.js --cache --cache-location=node_modules/.cache/.eslintcache",
    "lint:dev:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest --config ./eslint.config.stylistic.js",
    "lint:fix": "eslint -c ../../eslint.config.js --fix --cache --cache-location=node_modules/.cache/.eslintcache",
    "lint:inspector:clean": "rimraf .eslint-stylistic"
  },
  "dependencies": {
    "@chronoverse-shared/utilities": "^4.0.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "eslint": "^9.28.0"
  },
  "devDependencies": {
    "@chronoverse-shared/builder": "workspace:^",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": "^9.28.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.2.14",
  "engines": {
    "bun": ">=1.2.14",
    "node": ">=23.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
