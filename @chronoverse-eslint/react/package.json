{
  "name": "@chronoverse-eslint/react",
  "version": "3.0.3",
  "private": false,
  "description": "Comprehensive ESLint configuration for React projects with focus on best practices, accessibility, and performance",
  "keywords": [
    "eslint",
    "eslintconfig",
    "react",
    "typescript",
    "javascript",
    "jsx",
    "tsx",
    "a11y",
    "accessibility",
    "lint",
    "linter",
    "chronoverse",
    "performance",
    "best-practices"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gratisvictory/chronoverse-eslint"
  },
  "license": "MIT",
  "author": {
    "name": "@gratisvictory",
    "email": "gratisvictory.dev@proton.me",
    "url": "https://github.com/gratisvictory"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/@types/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/@types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run clean:all && bun build.js",
    "build:clean": "rimraf dist",
    "build:cli": "bun run clean:all && bun run builder -e src/index.js -o dist",
    "clean": "rimraf node_modules",
    "clean:all": "rimraf dist node_modules",
    "lint:build:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest build --config ./eslint.config.react.js --outDir .eslint-functional && bunx serve .eslint-react",
    "lint:dev:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest --config ./eslint.config.react.js",
    "lint:inspector:clean": "rimraf .eslint-react"
  },
  "dependencies": {
    "@chronoverse-shared/utilities": "^3.0.1",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "eslint": "^9.26.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.1-rc-af1b7da-20250421",
    "eslint-plugin-react-hooks": "^6.0.0"
  },
  "devDependencies": {
    "@chronoverse-shared/builder": "workspace:^",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": "^9.26.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.2.13",
  "engines": {
    "bun": ">=1.2.13",
    "node": ">=23.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
