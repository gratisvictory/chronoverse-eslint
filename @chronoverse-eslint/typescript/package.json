{
  "name": "@chronoverse-eslint/typescript",
  "version": "2.3.1",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/@types/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/@types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run clean:all && bun build.js",
    "build:clean": "rimraf dist",
    "build:cli": "bun run clean:all && bun run builder -e src/index.js",
    "clean": "rimraf node_modules",
    "clean:all": "rimraf dist node_modules"
  },
  "dependencies": {
    "@chronoverse-shared/utilities": "workspace:^",
    "@susisu/eslint-plugin-safe-typescript": "^0.9.4",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-tsdoc": "^0.4.0"
  },
  "devDependencies": {
    "@chronoverse-shared/builder": "workspace:^",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": "^9.26.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.2.11",
  "engines": {
    "bun": ">=1.2.11"
  },
  "publishConfig": {
    "access": "public"
  }
}
