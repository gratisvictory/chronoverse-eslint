{
  "name": "@chronoverse-eslint/functional",
  "version": "1.0.0",
  "private": false,
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/gratisvictory/chronoverse-eslint"
  },
  "license": "MIT",
  "author": {
    "name": "@gratisvictory",
    "email": "gratisvictory.dev@proton.me",
    "url": "https://github.com/gratisvictory"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/@types/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/@types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run clean:all && bun build.js",
    "build:clean": "rimraf dist",
    "build:cli": "bun run clean:all && bun run builder -e src/index.js -o dist",
    "clean": "rimraf node_modules",
    "clean:all": "rimraf dist node_modules",
    "lint:build:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest build --config ./eslint.config.functional.js --outDir .eslint-functional && bunx serve .eslint-functional",
    "lint:check": "eslint --cache --cache-location=node_modules/.cache/.eslintcache",
    "lint:debug": "eslint --cache --cache-location=node_modules/.cache/.eslintcache --debug",
    "lint:dev:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest --config ./eslint.config.functional.js",
    "lint:fix": "eslint --fix --cache --cache-location=node_modules/.cache/.eslintcache",
    "lint:inspector:clean": "rimraf .eslint-functional"
  },
  "dependencies": {
    "@chronoverse-shared/utilities": "workspace:^",
    "eslint": "^9.26.0",
    "eslint-plugin-functional": "^9.0.1"
  },
  "devDependencies": {
    "@chronoverse-shared/builder": "workspace:^",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": "^9.26.0"
  },
  "packageManager": "bun@1.2.13",
  "engines": {
    "bun": ">=1.2.13",
    "node": ">=23.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
