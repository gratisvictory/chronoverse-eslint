{
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint:check": {
      "dependsOn": [
        "build"
      ]
    },
    "lint:fix": {
      "dependsOn": [
        "build"
      ]
    },
    "format:check": {
      "dependsOn": [
        "build"
      ]
    },
    "format:fix": {
      "dependsOn": [
        "build"
      ]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "format:check",
          "format:fix",
          "lint:check",
          "lint:fix"
        ]
      }
    }
  }
}
