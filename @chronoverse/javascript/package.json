{
  "name": "@chronoverse-eslint/javascript",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build:clean && bun run build.js",
    "build:clean": "rimraf dist",
    "clean": "rimraf node_modules",
    "format:check": "prettier --check . --cache --cache-location=node_modules/.cache/.prettiercache",
    "format:fix": "prettier --write . --cache --cache-location=node_modules/.cache/.prettiercache --log-level=warn",
    "lint:build:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest build --config ./eslint.config.js --outDir .eslint && bunx serve .eslint",
    "lint:check": "eslint --cache --cache-location=node_modules/.cache/.eslintcache",
    "lint:dev:inspector": "bun run lint:inspector:clean && bunx @eslint/config-inspector@latest --config ./eslint.config.js",
    "lint:fix": "eslint --fix --cache --cache-location=node_modules/.cache/.eslintcache",
    "lint:inspector:clean": "rimraf .eslint",
    "lint:inspector:deploy": "bunx @eslint/config-inspector@latest build --config ./eslint.config.js --base /chronoverse-eslint/ --outDir .eslint"
  },
  "dependencies": {
    "eslint": "^9.25.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "esbuild": "^0.25.2",
    "eslint-config-flat-gitignore": "^2.1.0",
    "globals": "^16.0.0",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "eslint": "^9.25.1"
  },
  "packageManager": "bun@1.2.10",
  "engines": {
    "bun": ">=1.2.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
